{
  "name": "@onwidget/astrowind",
  "description": "AstroWind: A free template using Astro 4.0 and Tailwind CSS. Astro starter theme.",
  "version": "1.0.0-beta.22",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier -w .",
    "lint:eslint": "eslint . --ext .js,.ts,.astro",
    "sync:content": "vite-node scripts/sync.ts",
    "gen:schema": "openapi-zod-client http://0.0.0.0:8000/api/cms-schema/ -t templates/schemas.hbs -o src/schemas/content.ts && prettier -w src/schemas/content.ts",
    "gen:type": "openapi-zod-client http://0.0.0.0:8000/api/cms-schema/ -t templates/types.hbs -o src/types/content.ts",
    "gen:all": "npm run gen:schema && npm run gen:type"
  },
  "dependencies": {
    "@astrojs/react": "^3.1.1",
    "@astrojs/rss": "^4.0.5",
    "@astrojs/sitemap": "^3.1.2",
    "@astrolib/analytics": "^0.5.0",
    "@astrolib/seo": "^1.0.0-beta.5",
    "@fontsource-variable/inter": "^5.0.17",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "astro": "^4.5.15",
    "astro-icon": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "highlight.js": "^11.9.0",
    "limax": "4.1.0",
    "lodash": "^4.17.21",
    "lodash.isempty": "^4.4.0",
    "lodash.memoize": "^4.1.2",
    "lodash.merge": "^4.6.2",
    "lodash.setwith": "^4.3.2",
    "lucide-react": "^0.360.0",
    "marked": "^12.0.2",
    "marked-highlight": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss-animate": "^1.0.7",
    "unpic": "^3.18.0"
  },
  "devDependencies": {
    "@astrojs/mdx": "^2.2.3",
    "@astrojs/partytown": "^2.0.4",
    "@astrojs/tailwind": "5.1.0",
    "@iconify-json/flat-color-icons": "^1.1.10",
    "@iconify-json/tabler": "^1.1.109",
    "@tailwindcss/typography": "^0.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "astro-compress": "2.2.21",
    "eslint": "^8.57.0",
    "eslint-plugin-astro": "^0.33.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "js-yaml": "^4.1.0",
    "mdast-util-to-string": "^4.0.0",
    "openapi-zod-client": "^1.16.4",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "reading-time": "^1.5.0",
    "rehype-plugin-image-native-lazy-loading": "^1.2.0",
    "sharp": "0.33.3",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.3",
    "vite-node": "^1.6.0"
  },
  "engines": {
    "node": ">=18.14.1"
  }
}
